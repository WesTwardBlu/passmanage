	                <compilerArguments>
	                	<verbose />
						<bootclasspath>${java.home}\jre\lib\rt.jar;${java.home}\jre\lib\jce.jar</bootclasspath>
	                </compilerArguments>
	            </configuration>  
	        </plugin>  
